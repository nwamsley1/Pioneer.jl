{
    "paths": {
        "ms_data": "/path/to/ms/data/folder",
        "library": "/path/to/ms/data/library.pion",
        "results": "/path/to/results"
    },
    "output": {
        "write_csv": true,
        "write_decoys": false,
        "delete_temp": true,
        "plots_per_page": 12
    },
    "logging": {
        "debug_console_level": 0,
        "max_message_bytes": 4096
    },
    "global": {
        "scoring": {
            "q_value_threshold": 0.01
        },
        "match_between_runs": true,
        "ms1_quant": false
    },
    "first_search": {
        "fragment_settings": {
            "min_score": 15
        },
        "enable_irt_refinement": false
    },
    "acquisition": {
        "nce": 26,
        "quad_transmission": {
            "fit_from_data": true
        }
    },
    "maxLFQ": {
        "run_to_run_normalization": false
    }
}
