{
    "logging": {
        "debug_console_level": 0,
        "max_message_bytes": 4096
    },
    "global": {
        "isotope_settings": {
            "err_bounds_first_pass": [1, 0],
            "err_bounds_quant_search": [3, 0],
            "combine_traces": true,
            "partial_capture": true,
            "min_fraction_transmitted" : 0.25
        },
        "scoring": {
            "q_value_threshold": 0.01
        },
        "normalization": {
            "n_rt_bins": 100,
            "spline_n_knots": 7
        },
        "huber_override": {
            "override_huber_delta_fit": false,
            "huber_delta": 1055
        }, 
        "ms1_scoring": true,
        "ms1_quant": false,
		"match_between_runs": true
    },
    "parameter_tuning": {
        "fragment_settings": {
            "min_count": 7,
            "max_rank": 25,
            "min_score": [22, 17],
            "min_spectral_contrast": 0.5,
            "relative_improvement_threshold": 1.25,
            "min_log2_ratio": 1.5,
            "min_top_n": [3, 3],
            "n_isotopes": 1,
            "intensity_filter_quantile": 0.50
        },
        "search_settings": {
            "min_samples": 1200,
            "max_presearch_iters": 10,
            "frag_err_quantile": 0.005,
            "max_q_value": 0.01,
            "topn_peaks": 200,
            "max_frags_for_mass_err_estimation": 12
        },
        "nce_tuning": {
            "min_psms": 2000,
            "initial_percent": 2.5,
            "min_initial_scans": 5000
        },
        "quad_tuning": {
            "min_psms_per_thompson": 250,
            "min_fragments": 3,
            "initial_percent": 2.5
        },
        "iteration_settings": {
            "init_mass_tol_ppm": [20.0, 30.0],
            "ms1_tol_ppm": 20.0,
            "scan_counts": [10000]
        }
    },
    "first_search": {
        "fragment_settings": {
            "min_count": 4,
            "max_rank": 25,
            "min_score": 15,
            "min_spectral_contrast": 0.5,
            "relative_improvement_threshold": 1.25,
            "min_log2_ratio": 0.0,
            "min_top_n": [2, 3],
            "n_isotopes": 1
        },
        "scoring_settings": {
            "n_train_rounds": 2,
            "max_iterations": 20,
            "max_q_value_probit_rescore": 0.05,
            "max_PEP": 0.9
        },
        "irt_mapping": {
            "max_prob_to_impute_irt": 0.75,
            "fwhm_nstd": 4,
            "irt_nstd": 4,
            "plot_rt_alignment": true,
            "use_robust_fitting": true
        }
    },
    "quant_search": {
        "fragment_settings": {
            "min_count": 3,
            "min_y_count": 2,
            "max_rank": 255,
            "min_spectral_contrast": 0.0,
            "min_log2_ratio": -1.7,
            "min_top_n": [2, 3],
            "n_isotopes": 2
        },
        "chromatogram": {
            "smoothing_strength": 1e-6,
            "padding": 0,
            "max_apex_offset": 2
        }
    },
    "acquisition": {
        "nce": 26,
        "quad_transmission": {
            "fit_from_data": true,
            "overhang": 0.25,
            "smoothness": 5.0
        }
    },
    "rt_alignment": {
        "n_bins": 200,
        "bandwidth": 0.25,
        "sigma_tolerance": 4,
        "min_probability": 0.95,
        "lambda_penalty": 0.1,
        "ransac_threshold_psms": 500,
        "min_psms_for_spline": 10
    },
    "optimization": {
        "deconvolution": {
            "ms1": {
                "lambda": 0.0001,
                "reg_type": "l2",
                "huber_delta": 1e9
            },
            "ms2": {
                "lambda": 0.0,
                "reg_type": "none",
                "huber_delta": 300
            },
            "huber_exp": 1.5,
            "huber_iters": 15,
            "newton_iters": 50,
            "bisection_iters": 100,
            "outer_iters": 1000,
            "newton_accuracy": 10,
            "bisection_accuracy": 10,
            "max_diff": 0.01
        },
        "machine_learning": {
            "max_psms_in_memory": 50000000,
            "min_trace_prob": 0.75,
            "max_q_value_mbr_itr": 0.20,
            "min_PEP_neg_threshold_itr": 0.90,
            "spline_points": 500,
            "interpolation_points": 10,
            "n_quantile_bins": 25,
            "enable_model_comparison": true,
            "validation_split_ratio": 0.2,
            "qvalue_threshold": 0.01,
            "min_psms_for_comparison": 1000,
            "max_psms_for_comparison": 100000
        }
    },
    "proteinInference": {
        "min_peptides": 1
    },
    "maxLFQ": {
        "run_to_run_normalization": false
    },
    "output": {
        "write_csv": true,
        "write_decoys": false,
        "delete_temp": true,
        "plots_per_page": 12
    },
    "paths": {
        "library": "/path/to/lib.poin",
        "ms_data": "/path/to/msdata",
        "results": "/path/to/out_dir"
    }
}
